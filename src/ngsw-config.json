{
  "$schema": "https://raw.githubusercontent.com/angular/angular/main/packages/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/*.css",
          "/*.js",
          "/favicon.ico"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|png|webp|jpg|jpeg|gif|woff2|ttf)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [ "/api/**" ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "5s",
        "maxSize": 100,
        "maxAge": "1h"
      }
    }
  ]
}
